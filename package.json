    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "clean-webpack-plugin": "^3.0.0",
    "cross-spawn": "^6.0.5",
    "del": "^4.1.1",
    "fs": "0.0.1-security",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-hashsum": "^1.2.0",
    "gulp-if": "^2.0.2",
